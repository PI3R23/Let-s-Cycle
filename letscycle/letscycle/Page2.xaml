                HorizontalOptions="Center" />
            <ListView x:Name="chart" 
                HorizontalOptions="Center"
                VerticalOptions="Center">
                <ListView.ItemsSource>
                    <x:Array Type="{x:Type x:String}">
                        <x:String>KRYSTIAN GEJ 1</x:String>
                        <x:String>KRYSTIAN GEJ 2</x:String>
                        <x:String>KRYSTIAN GEJ 3</x:String>
